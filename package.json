{
  "name": "fractal-browser",
  "productName": "Fractal",
  "version": "0.0.1",
  "description": "AI-powered web browser that generates website content using Large Language Models",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "dev": "electron-forge start",
    "build": "electron-forge make",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist out .vite coverage",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "pre-push": "npm run type-check && npm run test"
  },
  "keywords": ["electron", "ai", "browser", "llm", "typescript", "vite"],
  "author": {
    "name": "Chris Cardoza",
    "email": "59742506+chcardoz@users.noreply.github.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron-forge/plugin-vite": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/ui": "^1.0.0",
    "electron": "37.3.1",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "playwright": "^1.40.0",
    "typescript": "~4.5.4",
    "vite": "^5.4.19",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.6",
    "@ai-sdk/google": "^2.0.8",
    "@ai-sdk/openai": "^2.0.19",
    "ai": "^5.0.0",
    "dotenv": "^17.2.1",
    "electron-squirrel-startup": "^1.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["biome check --write", "biome format --write"],
    "*.{json,md,yml,yaml}": ["biome format --write"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chcardoz/llm-browser.git"
  },
  "bugs": {
    "url": "https://github.com/chcardoz/llm-browser/issues"
  },
  "homepage": "https://github.com/chcardoz/llm-browser#readme"
}
